{
  "id": "arduino_pro_micro_5v_usbc",
  "name": "Arduino Pro Micro 5V (USB-C)",
  "description": "ATmega32U4 Pro Micro-compatible board (5V/16MHz, native USB, USB-C connector variant).",
  "platform": "avr",
  "manufacturer": "SparkFun",
  "target": "atmega32u4",
  "target_profile": "atmega32u4",
  "mcu": "ATmega32U4",
  "flash": "32KB",
  "ram": "2.5KB",
  "variant": {
    "clock_default_hz": 16000000
  },
  "capabilities": [
    "uart",
    "spi",
    "i2c",
    "gpio",
    "adc",
    "pwm",
    "timer",
    "eeprom",
    "remote_node",
    "mcp23017",
    "hp4067"
  ],
  "pins": {
    "uart_tx": 1,
    "uart_rx": 0,
    "i2c_sda": 2,
    "i2c_scl": 3,
    "spi_mosi": 16,
    "spi_miso": 14,
    "spi_sclk": 15,
    "spi_cs": 10,
    "led": 17,
    "button": -1
  },
  "pin_definitions": {
    "uart_tx": {
      "description": "UART TX (TXO)",
      "alternatives": [
        1
      ],
      "type": "output"
    },
    "uart_rx": {
      "description": "UART RX (RXI)",
      "alternatives": [
        0
      ],
      "type": "input"
    },
    "i2c_sda": {
      "description": "I2C SDA",
      "alternatives": [
        2
      ],
      "type": "bidirectional"
    },
    "i2c_scl": {
      "description": "I2C SCL",
      "alternatives": [
        3
      ],
      "type": "output"
    },
    "spi_mosi": {
      "description": "SPI MOSI",
      "alternatives": [
        16
      ],
      "type": "output"
    },
    "spi_miso": {
      "description": "SPI MISO",
      "alternatives": [
        14
      ],
      "type": "input"
    },
    "spi_sclk": {
      "description": "SPI SCK",
      "alternatives": [
        15
      ],
      "type": "output"
    },
    "spi_cs": {
      "description": "SPI CS/SS",
      "alternatives": [
        10
      ],
      "type": "output"
    },
    "led": {
      "description": "Onboard LED",
      "alternatives": [
        17
      ],
      "type": "output"
    }
  },
  "defaults": {
    "modules": [
      "gpio",
      "uart",
      "i2c",
      "spi",
      "timer",
      "shell_min"
    ],
    "clock": {
      "freq_hz": 16000000
    },
    "upload": {
      "method": "bootloader",
      "fqbn": "arduino:avr:micro",
      "mcu": "atmega32u4",
      "programmer": "avr109",
      "baud": 57600
    },
    "fuses": {
      "lfuse": "0xFF",
      "hfuse": "0xD8",
      "efuse": "0xCB",
      "lock": "0x2F"
    },
    "applets": [
      "blink",
      "uart_echo"
    ]
  },
  "notes": "USB-C connector variant does not change MCU pinout. Board package/FQBN may vary by vendor core.",
  "architecture": "AVR 8-bit",
  "family": "ATmega32U4"
}
